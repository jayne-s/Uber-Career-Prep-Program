//My Solution

class Solution {
public:
    vector<int> runningSum(vector<int>& nums) {
        
        vector<int> nums1 = nums;
    
        int sum = 0;
        int j = 1;
       for (int a = 0; a < nums.size(); a++){
        for (int i = 0; i < j; i++){
         sum += nums1[i];
        }
        j++;
        nums[a] = sum;
        sum = 0;  
       }
       return nums;
    }
};

//Optimal Solution

